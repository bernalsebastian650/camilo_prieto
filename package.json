  "dependencies": {
    "bootstrap-imageupload": "^1.1.3",
    "bootstrap-social": "^5.1.1",
    "copyfiles": "^2.4.1",
    "popper.js": "^1.16.1",
    "postcss": "^8.2.1",
    "sass": "^1.49.7",
    "sass-lint": "*",
    "uglify-js": "^3.12.2",
    "uglifyjs-folder": "^2.0.0"
